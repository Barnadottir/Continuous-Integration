{
  "name": "continuous-integration",
  "version": "1.0.0",
  "description": "CI Server",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "watch": "npx tsc -w",
    "start": "node dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "copy-files": "cp -r ./ci-results ./dist/.",
    "clean": "rm -r ./dist",
    "fresh-start": "npm run clean && npm run build && npm run copy-files && npm start"
